<h1>My coffees!</h1>

<table>
  <tr>
    <th>Title</th>

    <th></th>
    <th></th>
  </tr>

<% @coffeecups.each do |coffeecup| %>
  <tr>

    <td><%= link_to coffeecup.title, coffeecup %></td>
    <td><%= link_to 'Edit', edit_coffeecup_path(coffeecup) %></td>
    <td><%= link_to 'Destroy', coffeecup, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

